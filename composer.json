{
    "name": "gimmersta/model-types",
    "type": "library",
    "description": "Model types for Gimmersta",
    "keywords": ["gimmersta"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.12",
        "doctrine/dbal": "^2.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gimmersta\\ModelTypes\\": "src/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Gimmersta\\Cronos\\Core\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:gimmersta/storyblok-components.git"
        }
    ]
}
